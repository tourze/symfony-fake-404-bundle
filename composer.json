{
  "name": "tourze/symfony-fake-404-bundle",
  "description": "404 错误页面",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/framework-bundle": "^6.4",
    "symfony/twig-bundle": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Fake404Bundle\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}